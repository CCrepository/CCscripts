    <!--[START] Smart Dispo Suggestion [START] -->
    
     <script>
            
            var myVar8 ;
		function delayCALLvm() {
			myVar8 = setTimeout(callVM,1000)
;
			
		}
            
            
			function callVM() {
		
                /*--- Variable for dialing attempts ---*/
                var calls = document.getElementById("calls").innerHTML;
                				
				/*--- Variable to quickly grab our text box ---*/
				var text = document.getElementById("callVMprint");
                
                var VMscript = document.getElementById("VMscript").innerHTML;
				
				/*--- Figure out the current time ---*/
				
				if (calls == 1) {
					text.innerHTML = VMscript; }
                    
                    else {
                    text.innerHTML = "";
				}
			}
        
            
		</script>     
        
        
        
	
        
        
    
    
    
    
    
   


      
	


        
        
		<script>
            
            var myVar ;
		function func1() {
			myVar = setTimeout(insertDay,1000)
;
			
		}
            
            window.onload = delayCALLvm(); func1()
            
			function insertDay() {
		         
                /*--- Variable for dialing attempts ---*/
                var calls = document.getElementById("calls").innerHTML;
				
				/*--- Variable to quickly grab our text box ---*/
				var text = document.getElementById("dayBox");
				
				/*--- Figure out the current time ---*/
				var currentTime = new Date();
				var currentDay = currentTime.getDay();
				var currentHour = currentTime.getHours();
				
				/*--- Setup weekdays ---*/
				var weekDays = [
					'Sunday'
					,'Monday'
					,'Tuesday'
					,'Wednesday'
					,'Thursday'
					,'Friday'
					,'Saturday'];
				
				/*--- Actual function ---*/
                /*--- Zero Call ---*/
				if (calls == 0) {
					text.innerHTML = "CB-3min<br><br>New Berlin, WI <br>";
				}
				else {
                    
					/*--- Between Monday and Friday ---*/
					if (currentDay >= 1 && currentDay <= 5) {
						if (currentHour < 9) {
							text.innerHTML = "9am<br><br>New Berlin, WI <br>";
						}
						else if (currentHour >= 9 && currentHour < 13) {
							text.innerHTML = "1pm<br><br>New Berlin, WI <br>";
						}
						else if (currentHour >= 13 && currentHour < 17) {
							text.innerHTML = "5pm<br><br>New Berlin, WI <br>";
						}
						else {
							text.innerHTML = "9am<br><br>New Berlin, WI <br>";
						}
					}                        
				}
			}
            
		</script>




          
          <br>
          

         
