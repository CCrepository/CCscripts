			function insertDay() {
		
                /*--- Variable for dialing attempts ---*/
                var client = document.getElementById("client").innerHTML;
				
				/*--- Variable to quickly grab our text box ---*/
				var text = document.getElementById("dayBox");
		
				if (client == "ABW") {
					text.innerHTML = "ABW";
				}
				else {
                    
					/*--- Between Monday and Thursday ---*/
					if (client == "ABS") {
